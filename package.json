{
  "name": "@unclub/react-ui",
  "version": "1.0.0",
  "description": "Animated UI component library for developing gorgeous interfaces in react.",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook --docs -o ./docs",
    "prettier-format": "prettier --write \"**/*.{ts,tsx}\"",
    "build": "npm run build:clean && npm run build:lib",
    "build:clean": "rimraf dist",
    "build:lib": "rollup -c",
    "types": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "codecov": "codecov"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "collectCoverageFrom": [
    "src/**/*.{.ts,tsx}"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "javascript",
    "UI",
    "components"
  ],
  "author": "Ameya Upalanchi <uplanchiameya1144@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/AmeyaUpalanchi/unclub-react-ui.git"
  },
  "bugs": {
    "url": "https://github.com/AmeyaUpalanchi/unclub-react-ui/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/react": "^6.3.6",
    "@tailwindcss/postcss7-compat": "^2.2.7",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "add": "^2.0.6",
    "autoprefixer": "9",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^36.0.6",
    "eslint-plugin-prettier": "^3.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "jsdom": "^16.7.0",
    "postcss": "7",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.55.1",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-uglify": "^6.0.4",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.3.5",
    "yarn": "^1.22.11"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "tailwindcss": ">=2.0.0"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "postcss": "7"
  }
}